<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <!-- Dashboard Action -->
    <record id="action_dashboard" model="ir.actions.client">
        <field name="name">Need Dashboard</field>
        <field name="tag">odoo_dashboard</field>
    </record>

    <!-- Dashboard Menu -->
    <menuitem
        id="menu_dashboard"
        name="Need Dashboard"
        action="action_dashboard"
        sequence="1"
        groups="odoo_dashboard.group_dashboard_user"
        web_icon="odoo_dashboard,static/description/icon.png"/>

</odoo>
